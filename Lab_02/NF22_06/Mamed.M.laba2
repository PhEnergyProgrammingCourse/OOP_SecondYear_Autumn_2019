#include <iostream>
using namespace std;
 
int main() {
 double x0,x1,x2,x3,y0,y1,y2,y3;
 double z1,z2,z3;
 cin>>x0>>y0>>x1>>y1>>x2>>y2>>x3>>y3;
 z1 = (x1 - x0) * (y2 - y1) - (x2 - x1) * (y1 - y0);
 z2 = (x2 - x0) * (y3 - y2) - (x3 - x2) * (y2 - y0);
 z3 = (x3 - x0) * (y1 - y3) - (x1 - x3) * (y3 - y0);
 if ((z1>=0 && z2>=0 && z3>=0) || (z1<=0 && z2<=0 && z3<=0)) cout<<"0,1,2";
 else cout<<"0";
 return 0;
}
